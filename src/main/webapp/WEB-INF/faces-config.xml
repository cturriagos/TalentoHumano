<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/Views/Empresa/ciudad.xhtml</from-view-id>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/Empresa/didicacion.xhtml</from-view-id>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/cargo.xhtml</from-view-id>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/departamentos.xhtml</from-view-id>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/ingresoSalidas.xhtml</from-view-id>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/puestosLaborales.xhtml</from-view-id>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/HorarioLaboral/horarioLaboral.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DetalleHorario</from-outcome>
            <to-view-id>/Views/HorarioLaboral/detalleHorarioLaboral.xhtml</to-view-id>
            <redirect include-view-params="true"></redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>NuevoHorario</from-outcome>
            <to-view-id>/Views/HorarioLaboral/nuevoHorarioLaboral.xhtml</to-view-id>
            <redirect include-view-params="true"></redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/HorarioLaboral/detalleHorarioLaboral.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>HorarioLaboral</from-outcome>
            <to-view-id>/Views/HorarioLaboral/horarioLaboral.xhtml</to-view-id>
            <redirect include-view-params="true"></redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/HorarioLaboral/nuevoHorarioLaboral.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>HorarioLaboral</from-outcome>
            <to-view-id>/Views/HorarioLaboral/horarioLaboral.xhtml</to-view-id>
            <redirect include-view-params="true"></redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Views/diaSemana.xhtml</from-view-id>
    </navigation-rule>
</faces-config>
